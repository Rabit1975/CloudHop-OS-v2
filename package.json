{
  "name": "cloudhop-os-v2",
  "version": "2.0.0",
  "description": "CloudHop 2.0 - A modular operating system for next-generation communication",
  "main": "dist-electron/main.js",
  "scripts": {
    "start": "concurrently \"npm run dev\" \"npm run electron:start\"",
    "dev": "vite --host 127.0.0.1",
    "build": "tsc && tsc -p tsconfig.electron.json && vite build",
    "electron:build": "tsc -p tsconfig.electron.json",
    "electron:start": "npm run electron:build && electron .",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "cloudhop",
    "os",
    "electron",
    "geforce-now"
  ],
  "author": "CloudHop Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@react-three/fiber": "^8.15.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/three": "^0.160.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "electron": "^30.5.1",
    "typescript": "^5.0.0",
    "vite": "^7.3.1"
  }
}
